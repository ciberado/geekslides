{
  "name": "geekslides-engine",
  "version": "0.1.0",
  "description": "html based slideshow, without any I don't Need It features.",
  "default": "index.html",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html --no-source-maps",
    "test": "jest --watch"
  },
  "author": "Javi <email@javier-moreno.com>",
  "license": "GNU General Public License v3.0",
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "chart.js": "^2.9.4",
    "color-math": "^1.1.3",
    "d3": "^6.1.1",
    "i": "^0.3.7",
    "markdown-it": "^13.0.1",
    "markdown-it-block-image": "^0.0.3",
    "markdown-it-container": "^3.0.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-highlightjs": "^4.0.1",
    "notie": "^4.3.1",
    "npm": "^8.9.0",
    "paho-mqtt": "^1.1.0",
    "regenerator-runtime": "^0.13.5",
    "scripts": "^0.1.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-private-methods": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.6",
    "babel-jest": "^25.5.1",
    "emmet": "^2.0.0",
    "jest": "^28.1.0",
    "marked": "^4.0.15",
    "parcel": "^2.5.0",
    "parcel-plugin-custom-dist-structure": "^1.1.18",
    "parcel-plugin-imagemin": "^4.0.1",
    "parcel-plugin-static-files-copy": "^2.3.1",
    "path-browserify": "^1.0.1"
  },
  "staticFiles": {
    "staticPath": [],
    "watcherGlob": "**"
  },
  "customDistStructure": {
    "config": {
      "slide-assets": [
        ".js",
        ".css",
        ".jpg"
      ]
    },
    "options": {
      "development": true
    }
  }
}
