{
  "name": "geekslides-engine",
  "version": "0.1.0",
  "description": "html based slideshow, without any I don't Need It features.",
  "default": "index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html --no-source-maps",
    "test": "jest --watch"
  },
  "author": "Javi <email@javier-moreno.com>",
  "license": "GNU General Public License v3.0",
  "dependencies": {
    "markdown-it": "^13.0.1",
    "markdown-it-block-image": "^0.0.3",
    "markdown-it-container": "^3.0.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-highlightjs": "^4.0.1",
    "notie": "^4.3.1",
    "paho-mqtt": "^1.1.0",
    "scripts": "^0.1.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "emmet": "^2.0.0",
    "jest": "^28.1.0",
    "marked": "^4.0.15",
    "parcel": "latest",
    "parcel-plugin-custom-dist-structure": "^1.1.18",
    "parcel-plugin-imagemin": "^4.0.1",
    "parcel-plugin-static-files-copy": "^2.3.1",
    "path-browserify": "^1.0.1"
  },
  "staticFiles": {
    "staticPath": [],
    "watcherGlob": "**"
  },
  "customDistStructure": {
    "config": {
      "slide-assets": [
        ".js",
        ".css",
        ".jpg"
      ]
    },
    "options": {
      "development": true
    }
  }
}
